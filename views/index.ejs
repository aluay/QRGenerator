                    <label class="phoneQRPatternColorLabel" for="phoneQRPatternColor">Pattern</label>
                    <input type="color" id="phoneQRPatternColor" name="phoneQRPatternColor" value="#000000">
                    <label class="phoneQRBGColorLabel" for="phoneQRBGColor">Background</label>
                    <input type="color" id="phoneQRBGColor" name="phoneQRBGColor" value="#FFFFFF">
                </div>
                </form>
            </div>
            <div class="tab" id="cryptoQR" style="display: none;">
