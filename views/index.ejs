                    <div class="qrColorsDiv">
                        <label class="vcardQRPatternColorLabel" for="vcardQRPatternColor">Pattern</label>
                        <input type="color" id="vcardQRPatternColor" name="vcardQRPatternColor" value="#000000">
                        <label class="vcardQRBGColorLabel" for="vcardQRBGColor">Background</label>
                        <input type="color" id="vcardQRBGColor" name="vcardQRBGColor" value="#FFFFFF">
                    </div>
                </form>
            </div>
