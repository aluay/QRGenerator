                    <label class="emailQRPatternColorLabel" for="emailQRPatternColor">Pattern</label>
                    <input type="color" id="emailQRPatternColor" name="emailQRPatternColor" value="#000000">
                    <label class="emailQRBGColorLabel" for="emailQRBGColor">Background</label>
                    <input type="color" id="emailQRBGColor" name="emailQRBGColor" value="#FFFFFF">
                </div>
                </form>
            </div>
            <div class="tab" id="phoneQR" style="display: none;">
                <form method="post">
                    <div id="phoneQR" class="phone">
                    <input type="text" name="phone" class="phone" id="phone" placeholder="Phone Number">
                </div>
