                        <label class="urlQRPatternColorLabel" for="urlQRPatternColor">Pattern</label>
                        <input type="color" id="urlQRPatternColor" name="urlQRPatternColor" value="#000000">
                        <label class="urlQRBGColorLabel" for="urlQRBGColor">Background</label>
                        <input type="color" id="urlQRBGColor" name="urlQRBGColor" value="#FFFFFF">
                    </div>
                </form>
            </div>
